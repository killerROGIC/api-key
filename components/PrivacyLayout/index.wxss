.flex-around-box,.flex-around-box-center,.flex-between-box,.flex-between-box-center,.flex-box,.flex-box-center,.flex-center-box,.flex-column-box {
    display: -webkit-flex;
    display: flex
}

.flex-box-center,.flex-column-box {
    -webkit-align-items: center;
    align-items: center
}

.flex-column-box {
    -webkit-flex-direction: column;
    flex-direction: column
}

.flex-between-box,.flex-between-box-center {
    -webkit-justify-content: space-between;
    justify-content: space-between
}

.flex-around-box,.flex-around-box-center {
    -webkit-justify-content: space-around;
    justify-content: space-around
}

.flex-around-box-center,.flex-between-box-center,.flex-center-box {
    -webkit-align-items: center;
    align-items: center
}

.flex-center-box {
    -webkit-justify-content: center;
    justify-content: center
}

